
<div>
    <h1><%= title %></h1>
    <%- messages() %>
    <form action="/account/login" method="POST" id="login-form">
        <label for="email">Email:</label>
        <input type="email" id="email" name="account_email" required />

        <label for="password">Password:</label>
        <input type="password" id="password" name="account_password" required />

        <button type="submit">Login</button> 
    </form>

    <p>Don't have an account? <a href="/account/register">Sign-up</a>.</p>
</div>  


